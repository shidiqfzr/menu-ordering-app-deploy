{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "uploads/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server.js" },
      { "src": "/images/(.*)", "dest": "/uploads/$1" }
    ]
  }
  